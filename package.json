{
  "name": "angular2-state-machine",
  "version": "0.0.5",
  "description": "Angular 2 State Machine - Angular 2 port for famous state-machine. https://github.com/jakesgordon/javascript-state-machine.",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "karma start",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "main": "src/core.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aminbh111/angular2-state-machine.git"
  },
  "keywords": [
    "Angular2",
    "State",
    "Machine",
    "handling",
    "states",
    "states",
    "state",
    "machine",
    "ng-state-machine",
    "ng2",
    "angular2",
    "port"
  ],
  "author": "Vadym Kukhtin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aminbh111/angular2-state-machine/issues"
  },
  "module": "index.js",
  "homepage": "https://github.com/aminbh111/angular2-state-machine#readme",
  "devDependencies": {
    "@angular/common": "~8.0.0-beta.10",
    "@angular/core": "~8.0.0-beta.10",
    "@angular/http": "~8.0.0-beta.10",
    "@angular/platform-browser": "~8.0.0-beta.10",
    "@types/jasmine": "~3.3.8",
    "@types/js-base64": "^2.1.3",
    "awesome-typescript-loader": "^2.2.4",
    "core-js": "^2.3.0",
    "es6-shim": "^0.35.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "prettier": "^1.18.2",
    "reflect-metadata": "0.1.8",
    "rxjs": "^6.4.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.3",
    "webpack": "^1.13.0",
    "zone.js": "~0.9.1"
  }
}
